{
  "name": "@koakh/nestjs-surrealdb-rest-demo",
  "version": "0.1.0",
  "description": "SurrealDb NestJs wrapper driver - Consumer app",
  "author": "MÃ¡rio Monteiro <marioammonteiro@gmail.com>",
  "readmeFilename": "README.md",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --debug --watch",
    "start:debug": "npm run kill-port || true && nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main",
    "kill-port": "sudo fuser -k 3030/tcp",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:build": "docker-compose build",
    "docker:push": "docker push koakh/nestjs-surrealdb-rest-demo:1.0.0",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down --remove-orphans"
  },
  "// opt local/remote @koakh/nestjs-surrealdb": {
    "local": "file:../app-lib",
    "remote ": "^0.1.2"
  },
  "// use @koakh/nestjs-surrealdb 0.1.3 before add graphql stuff, don't add ^": "",
  "dependencies": {
    "@koakh/nestjs-surrealdb": "workspace:^",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "babel-core": "^6.26.3",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.0",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
