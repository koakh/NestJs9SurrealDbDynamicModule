{
  "name": "@koakh/nestjs-surrealdb",
  "version": "0.1.8",
  "description": "SurrealDb NestJs wrapper driver",
  "author": "MÃ¡rio Monteiro <marioammonteiro@gmail.com>",
  "license": "UNLICENSED",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "version:patch": "npm run build && npm version patch && echo 'don`t forget to bump version on consumer apps and run `yarn install --mode=skip-build`'",
    "version:publish": "npm publish"
  },
  "keywords": [
    "nestjs",
    "surrealdb"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koakh/NestJs9SurrealDbDynamicModule.git"
  },
  "bugs": "https://github.com/koakh/NestJs9SurrealDbDynamicModule.git",
  "dependencies": {
    "@golevelup/nestjs-modules": "0.7.1",
    "@nestjs/config": "3.3.0",
    "@nestjs/graphql": "12.2.1",
    "babel-core": "6.26.3",
    "class-validator": "^0.14.1",
    "graphql-subscriptions": "3.0.0",
    "rxjs": "7.8.1",
    "surrealdb": "^1.1.0",
    "ts-morph": "24.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "10.4.9",
    "@nestjs/core": "10.4.9",
    "@nestjs/testing": "10.4.9",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.0",
    "jest": "29.7.0",
    "nodemon": "3.1.7",
    "prettier": "3.4.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsc-watch": "6.3.0",
    "tsconfig-paths": "4.2.0",
    "tslint": "^6.1.3",
    "typescript": "5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
