{
  "name": "@koakh/nestjs-surrealdb",
  "version": "0.1.3",
  "description": "SurrealDb NestJs wrapper driver",
  "author": "MÃ¡rio Monteiro <marioammonteiro@gmail.com>",
  "license": "UNLICENSED",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "version:patch": "npm run build && npm version patch",
    "version:publish": "npm publish"
  },
  "keywords": [
    "nestjs",
    "surrealdb"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koakh/NestJs9SurrealDbDynamicModule.git"
  },
  "bugs": "https://github.com/koakh/NestJs9SurrealDbDynamicModule.git",
  "peerDependencies": {
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2"
  },
  "dependencies": {
    "@golevelup/nestjs-modules": "^0.6.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/graphql": "^10.1.2",
    "babel-core": "^6.26.3",
    "class-validator": "^0.13.2",
    "graphql-subscriptions": "^2.0.0",
    "surrealdb.js": "^0.5.0",
    "ts-morph": "^16.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "@nestjs/testing": "^9.0.11",
    "@types/jest": "^26.0.24",
    "@types/node": "^18.7.16",
    "jest": "27.0.6",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "27.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
