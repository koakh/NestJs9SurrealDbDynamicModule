{
  "name": "root",
  "private": true,
  "devDependencies": {
    "npm-check": "^6.0.1",
    "npm-check-updates": "^16.1.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "yarn:install": "yarn",
    "yarn:start:app-lib:dev": "yarn --cwd packages/app-lib dev",
    "yarn:start:app-lib:build": "yarn --cwd packages/app-lib build",
    "yarn:start:app-lib:patch": "yarn --cwd packages/app-lib version:patch",
    "yarn:start:app-lib:publish": "yarn --cwd packages/app-lib version:publish",
    "yarn:start:app-rst": "yarn --cwd packages/app-gql start:debug",
    "yarn:start:app-gql": "yarn --cwd packages/app-gql start:debug"
  }
}